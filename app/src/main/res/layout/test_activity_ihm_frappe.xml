<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@color/blanc">

    <com.androidplot.xy.XYPlot
        android:id="@+id/precisionPlot"
        android:layout_width="300dp"
        android:layout_height="300dp"
        androidPlot.title="Precision"
        android:layout_alignParentRight="true"
        adroidPlot.titleWidget.labelPaint.textSize="40dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/textData"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        />

    <com.androidplot.xy.XYPlot
        android:id="@+id/forcePlot"
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        androidPlot.title="Force"
        android:layout_below="@id/precisionPlot"
        droidPlot.titleWidget.labelPaint.textSize="20dp"/>

    <com.androidplot.xy.XYPlot
        android:id="@+id/vitessePlot"
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        androidPlot.title="Vitesse"
        android:layout_below="@id/forcePlot"
        droidPlot.titleWidget.labelPaint.textSize="20dp"
        />

